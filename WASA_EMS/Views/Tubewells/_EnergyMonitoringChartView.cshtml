
<style>
    div#chartContainer1 {
        position: relative;
    }

        div#chartContainer1:after {
            content: "";
            display: block;
            position: absolute;
            bottom: 0;
            left: 0;
            background: #fff;
            width: 57px;
            height: 14px;
        }

    a.canvasjs-chart-credit {
        display: none;
    }
</style>
<div id="chartContainer1" style="width: 95%; height: 500px"></div>
<script src="http://canvasjs.com/assets/script/canvasjs.min.js" type="text/javascript"></script>
<script type="text/javascript">
    alert('call');
    console.log(@ViewData["chartData"]);
    $(document).ready(function () {
        @Html.Raw(ViewData["chartData"])
        chart1.render();

        function toogleDataSeries(e) {
            if (typeof (e.dataSeries.visible) === "undefined" || e.dataSeries.visible) {
                e.dataSeries.visible = false;
            } else {
                e.dataSeries.visible = true;
            }
            chart1.render();
        }
    });
</script>